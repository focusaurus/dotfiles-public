act(d, down, volume -10%)
act(e, 80%, volume 80%)
act(f, 40%, volume 40%)
act(m, mute, volume 0)
act(n, 90%, volume 90%)
act(o, 100%, volume 100%)
act(h, 100%, volume 100%)
act(s, 60%, volume 60%)
act(t, toggle mute, volume-toggle-mute)
act(u, up, volume +10%)
