{
  "telemetry.telemetryLevel": "off",
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "workbench.tree.indent": 20,
  "workbench.startupEditor": "newUntitledFile",
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "hadolint.hadolintPath": "/usr/local/bin/hadolint",
  "macros.list": {
    "sort-to-top": [
      "bookmarks.toggle",
      {
        "command": "workbench.action.gotoLine",
        "args": {
          "line": "1"
        }
      },
      "bookmarks.expand-selection-to-next"
    ]
  },
  "files.autoSave": "onFocusChange",
  "breadcrumbs.enabled": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.occurrencesHighlight": false,
  "git.confirmSync": false,
  "editor.tabCompletion": "on",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "window.newWindowDimensions": "maximized",
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  "window.title": "${folderName}${separator}${activeEditorShort}${separator}${appName}",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[rs]": {
    "editor.tabSize": 4
  },
  "[php]": {
    "editor.tabSize": 4,
    "editor.detectIndentation": false
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "calva.paredit.defaultKeyMap": "none",
  "explorer.confirmDelete": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.renderWhitespace": "none",
  "editor.acceptSuggestionOnEnter": "off",
  "shellcheck.disableVersionCheck": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "editor.fontFamily": "'JetBrains Mono', 'Inconsolata', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "markdown.preview.markEditorSelection": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "gitlens.advanced.telemetry.enabled": false,
  "workbench.activityBar.visible": true,
  "terminal.integrated.allowChords": false,
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "keyboard.dispatch": "keyCode",
  "editor.wordWrapColumn": 80,
  "search.enableSearchEditorPreview": true,
  "calva.fmt.formatAsYouType": false,
  "calva.fmt.insertMissingWhitespace": false,
  "calva.fmt.indentation": false,
  "calva.fmt.removeSurroundingWhitespace": false,
  "calva.fmt.removeTrailingWhitespace": false,
  "autofold.types": [
    {
      "ext": ".jsOFF",
      "level": 1
    },
    {
      "ext": ".cljOFF",
      "level": 1,
    }
  ],
  "workbench.editor.showTabs": true,
  "editor.cursorWidth": 2,
  "editor.renderLineHighlight": "all",
  "editor.foldingHighlight": false,
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "go.formatFlags": [
    "-local git.rsglab.com"
  ],
  "gopls": {
    "local": "git.rsglab.com"
  },
  "protoc": {
    "options": [
      "-I",
      "./proto"
    ]
  },
  "go.toolsManagement.autoUpdate": true,
  "go.lintTool": "golangci-lint",
  "intelephense.telemetry.enabled": false,
  "editor.scrollBeyondLastLine": false,
  "outline.showArrays": false,
  "outline.showConstants": false,
  "outline.showEnums": false,
  "outline.showNumbers": false,
  "outline.showPackages": false,
  "outline.showStrings": false,
  "outline.showStructs": false,
  "outline.showVariables": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "web/_domains/list-manage/release/__dev__static/**": true
  },
  "java.imports.gradle.wrapper.checksums": [
    {
      "sha256": "20cb91d1f40aa1e56e5e9406328e91fa9e37bd6459a705b96304b57bcd5b32b1",
      "allowed": false
    }
  ],
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Kimbie Dark"
}
