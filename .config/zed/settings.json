{
  "extend_comment_on_newline": false,
  "completions": {
    "lsp": false
  },
  "inlay_hints": {
    "show_value_hints": false
  },
  "edit_predictions": {
    "mode": "subtle"
  },
  "show_edit_predictions": false,
  "show_whitespaces": "boundary",
  "jsx_tag_auto_close": {
    "enabled": false
  },
  "show_completions_on_input": true,
  "use_auto_surround": false,
  "use_autoclose": false,
  "format_on_save": "off",
  "agent": {
    "default_profile": "ask",
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-latest"
    },
    "model_parameters": []
  },
  "autosave": "on_window_change",
  "telemetry": {
    "diagnostics": true,
    "metrics": true
  },
  "formatter": null,
  "vim_mode": true,
  "ui_font_size": 14,
  "buffer_font_size": 14,
  "languages": {
    "Go": {
      "tab_size": 2,
      "format_on_save": "on"
    },
    "JavaScript": {
      "format_on_save": "off"
    },
    "TypeScript": {
      "format_on_save": "off"
    }
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "One Dark"
  }
}
